<h2>There and Back Again</h2>
<h3>a mood tracking journal adventure</h3>
<h4>Login</h4>


<%= form_tag("/session", method: "post") do %>
  <%= label_tag(:email) %>
  <%= text_field_tag(:email) %>
  <%= label_tag(:password) %>
  <%= password_field_tag(:password) %>

  <%= submit_tag("Enter") %>
<% end %>